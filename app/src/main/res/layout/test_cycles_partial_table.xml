        >
        <TextView
            style="@style/cycle_constant"
            android:text="3"
            />
        <include layout="@layout/test_cycle_partial_parameters"/>
    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <TextView
            style="@style/cycle_constant"
            android:text="4"
            />
        <include layout="@layout/test_cycle_partial_parameters"/>
    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <TextView
            style="@style/cycle_constant"
            android:text="5"
            />
        <include layout="@layout/test_cycle_partial_parameters"/>
    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <TextView
            style="@style/cycle_constant"
            android:text="6"
            />
        <include layout="@layout/test_cycle_partial_parameters"/>
    </TableRow>

